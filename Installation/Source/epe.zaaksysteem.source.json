            "zql": "SELECT%20case.status%2C%20case.num_unaccepted_updates%2C%20case.num_unaccepted_files%2C%20case.num_unread_communication%2C%20case.number%2C%20case.progress_status%2C%20case.casetype.name%2C%20case.subject%2C%20attribute.woo_publicatiedatum%2C%20attribute.woo_categorie%2C%20case.assignee%2C%20case.date_of_registration%2C%20case.requestor.name%2C%20case.days_left%2C%20case.newtab%2C%20case.archival_state%2C%20case.destructable%2C%20case.date_target%20FROM%20case%20WHERE%20(case.casetype.id%20IN%20(5382%2C128))%20ORDER%20BY%20case.number%20DESC"
        }
    },
    "isEnabled": false
}