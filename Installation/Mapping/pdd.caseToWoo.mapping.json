{
  "title": "CaseToWooObject",
  "$id": "https://commongateway.nl/mapping/pdd.CaseToWooObject.mapping.json",
  "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
  "version": "0.0.1",
  "passTrough": false,
  "mapping": {
    "UUID": "reference",
    "ID": "reference",
    "Object_ID": "reference",
    "Portal_url": "unknown",
    "Behandelend_bestuursorgaan": "unknown",
    "Ontvanger_informatieverzoek": "unknown",
    "Volgnummer": "instance.number",
    "Titel": "instance.casetype.preview",
    "Beschrijving": "unknown",
    "Samenvatting": "unknown",
    "Verzoeker": "instance.requestor.preview",
    "Ontvangstdatum": "instance.date_created",
    "Besluitdatum": "unknown",
    "Behandelstatus": "instance.phase",
    "Besluit": "unknown",
    "Termijnoverschrijding": "unknown",
    "URL_informatieverzoek":"unknown",
    "URL_inventarisatielijst": "unknown",
    "URL_besluit": "unknown",
    "Geografisch_gebied": "unknown",
    "BAG_ID": "instance.requestor.instance.subject.instance.address_residence.instance.bag_id",
    "BGT_ID": "unknown",
    "Postcodegebied": "unknown",
    "Adres": "{% if instance.requestor.instance.subject.instance.address_residence.instance.street|default and instance.requestor.instance.subject.instance.address_residence.instance.street_number|default %}{{ instance.requestor.instance.subject.instance.address_residence.instance.street~' '~instance.requestor.instance.subject.instance.address_residence.instance.street_number  }}{% endif %}",
    "COORDS": "unknown",
    "Geografische_positie": "unknown",
    "Bijlagen": "unknown (could be fetched with a extra request)",
    "Themas": "unknown"
  }
}