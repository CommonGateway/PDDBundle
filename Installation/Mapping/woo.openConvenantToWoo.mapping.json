{
  "title": "OpenConvenantToWoo",
  "$id": "https://commongateway.nl/mapping/woo.openConvenantToWoo.schema.json",
  "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
  "version": "0.0.1",
  "passTrough": false,
  "mapping": {
    "_sourceId": "ID",
    "id": "identifier",
    "behandelendBestuursorgaan.oidn": "{% if behandelendBestuursorgaan.oidn|default %}{{ behandelendBestuursorgaan.oidn }}{% else %}unknown{% endif %}",
    "behandelendBestuursorgaan.naam": "behandelendBestuursorgaan.naam",
    "categorie": "categorie",
    "titel": "{% if Onderwerp|default %}{{ Onderwerp }}{% endif %}",
    "samenvatting": "{% if Samenvatting|default %}{{ Samenvatting }}{% endif %}",
    "ontvangstdatum": "",
    "publicatiedatum": "{{ \"now\"|date(\"Y-m-d H:i:s\") }}",
    "metadata.besluitdatum": "{% if Datum_ondertekening|default %}{{ Datum_ondertekening }}{% endif %}",
    "behandelstatus": "",
    "termijnoverschrijding": "",
    "urlInformatieverzoek": "",
    "urlInventarisatielijst": "",
    "urlBesluit": "",
    "informatieverzoek": "",
    "besluit": "",
    "bijlagen": "[{% set index=0 %}{% if Bijlagen|default %}{% for bijlage in Bijlagen %}{% if index != 0 %},{% endif %}{{ map('https://commongateway.nl/mapping/woo.openConvenantBijlageToWooBijlage.mapping.json', bijlage)|json_encode }}{% set index=index+1 %}{% endfor %}{% endif %}]",
    "themas.0.hoofdthema": "{% if Beleidsterrein|default %}{{ Beleidsterrein }}{% endif %}"
  },
  "cast": {
    "id": "string",
    "themas.0.hoofdthema": "unsetIfValue==",
    "themas.0": "unsetIfValue==",
    "informatieverzoek": "jsonToArray",
    "inventarisatielijst": "jsonToArray",
    "besluit": "jsonToArray",
    "bijlagen": "jsonToArray",
    "portalUrl": "unsetIfValue==",
    "titel": "unsetIfValue==",
    "beschrijving": "unsetIfValue==",
    "samenvatting": "unsetIfValue==",
    "ontvangstdatum": "unsetIfValue==",
    "termijnoverschrijding": "unsetIfValue==",
    "behandelstatus": "unsetIfValue==",
    "metadata.besluitdatum": "unsetIfValue==",
    "categorie": "unsetIfValue==",
    "publicatiedatum": "unsetIfValue==",
    "urlInformatieverzoek": "unsetIfValue==",
    "urlInventarisatielijst": "unsetIfValue==",
    "urlBesluit": "unsetIfValue=="
  }
}
