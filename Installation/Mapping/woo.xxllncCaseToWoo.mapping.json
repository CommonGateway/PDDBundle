{
    "title": "XxllncCaseToWoo",
    "$id": "https://commongateway.nl/mapping/pdd.xxllncCaseToWoo.schema.json",
    "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
    "version": "0.0.4",
    "passTrough": false,
    "mapping": {
        "_sourceId": "id",
        "oidn": "{% if oidn|default %}{{ oidn }}{% else %}unknown{% endif %}",
        "UUID": "case.class_uuid",
        "ID": "id",
        "Object_ID": "object_id",
        "Portal_url": "",
        "Behandelend_bestuursorgaan": "",
        "Ontvanger_informatieverzoek": "",
        "Volgnummer": "",
        "Titel": "values.case&#46;casetype&#46;name",
        "Beschrijving": "values.case&#46;subject",
        "Samenvatting": "values.case&#46;subject",
        "Categorie": "values.case&#46;casetype&#46;name",
        "Verzoeker": "values.attribute&#46;woo_categorie",
        "Ontvangstdatum": "{% if values['attribute&#46;woo_publicatiedatum']|default %}{{ values['attribute&#46;woo_publicatiedatum'] }}{% else %}{{ 'now'|date('Y-m-dTH:i:s') }}{% endif %}",
        "Besluitdatum": "values.case&#46;date_target",
        "Behandelstatus": "values.case&#46;status",
        "Besluit": "",
        "Termijnoverschrijding": "",
        "URL_informatieverzoek": "",
        "URL_inventarisatielijst": "",
        "URL_besluit": "",
        "Geografisch_gebied": "",
        "BAG_ID": "",
        "BGT_ID": "",
        "Postcodegebied": "",
        "Adres": "",
        "COORDS": "",
        "Geografische_positie": "",
        "Bijlagen": "[{% set index=0 %}{% if values['attribute&#46;test_documenten']|default %}{% for document in values['attribute&#46;test_documenten'] %}{% if index != 0 %},{% endif %}{{ map('https://commongateway.nl/mapping/pdd.xxllncTestDocumentToBijlage.schema.json', document)|json_encode }}{% set index=index+1 %}{% endfor %}{% endif %}]",
        "Themas": ""
    },
    "cast": {
        "Bijlagen": "jsonToArray",
        "Object_ID": "integer"
    }
}