{
  "title": "ZaakToWoo",
  "$id": "https://commongateway.nl/mapping/woo.zaakToWoo.mapping.json",
  "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
  "version": "0.0.1",
  "passTrough": false,
  "mapping": {
    "_sourceId": "id",
    "id": "object_id",
    "organisatie.oin": "{% if organisatie.oin|default %}{{ organisatie.oin }}{% else %}unknown{% endif %}",
    "organisatie.naam": "organisatie.naam",
    "titel": "{% if omschrijving|default %}{{ omschrijving }}{% endif %}",
    "beschrijving": "{% if eigenschappen.woo_beschrijving|default %}{{ eigenschappen.woo_beschrijving }}{% endif %}",
    "samenvatting": "{% if eigenschappen.woo_samenvatting|default %}{{ eigenschappen.woo_samenvatting }}{% endif %}",
    "categorie": "{% if eigenschappen.woo_categorie|default %}{{ eigenschappen.woo_categorie }}{% endif %}",
    "metadata.verzoek.ontvangstdatum": "{% if eigenschappen.date_of_registration|default %}{{ eigenschappen.date_of_registration }}{% endif %}",
    "metadata.besluitdatum": "{% if eigenschappen.woo_datum_besluit|default %}{{ eigenschappen.woo_datum_besluit }}{% endif %}",
    "publicatiedatum": "{% if eigenschappen.woo_publicatiedatum|default %}{{ eigenschappen.woo_publicatiedatum }}{% endif %}",
    "metadata.verzoek.termijnoverschrijding": "",
    "metadata.verzoek.informatieverzoek": "{% if enkelvoudigInformatieObjecten['informatieverzoek'][0]|default %}{{ map('https://commongateway.nl/mapping/woo.zgwInformatieObjectSetSourceId.schema.json', enkelvoudigInformatieObjecten['informatieverzoek'][0])|json_encode }}{% endif %}",
    "metadata.verzoek.inventarisatielijst": "{% if enkelvoudigInformatieObjecten['inventarisatielijst'][0]|default %}{{ map('https://commongateway.nl/mapping/woo.zgwInformatieObjectSetSourceId.schema.json', enkelvoudigInformatieObjecten['inventarisatielijst'][0])|json_encode }}{% endif %}",
    "metadata.verzoek.besluit": "{% if enkelvoudigInformatieObjecten['besluit'][0]|default %}{{ map('https://commongateway.nl/mapping/woo.zgwInformatieObjectSetSourceId.schema.json', enkelvoudigInformatieObjecten['besluit'][0])|json_encode }}{% endif %}",
    "bijlagen": "[{% set index=0 %}{% if enkelvoudigInformatieObjecten['bijlagen']|default %}{% for document in enkelvoudigInformatieObjecten['bijlagen'] %}{% if index != 0 %},{% endif %}{{ map('https://commongateway.nl/mapping/woo.zgwInformatieObjectSetSourceId.schema.json', document)|json_encode }}{% set index=index+1 %}{% endfor %}{% endif %}]",
    "themas.0.hoofdthema._sourceId": "{% if eigenschappen.woo_thema']|default %}{{ eigenschappen.woo_thema'] }}{% endif %}",
    "themas.0.hoofdthema": "{% if eigenschappen.woo_thema']|default %}{{ eigenschappen.woo_thema'] }}{% endif %}"
  },
  "cast": {
    "id": "string",
    "themas.0.hoofdthema": "unsetIfValue==",
    "themas.0": "unsetIfValue==",
    "bijlagen": "jsonToArray",
    "portalUrl": "unsetIfValue==",
    "titel": "unsetIfValue==",
    "beschrijving": "unsetIfValue==",
    "samenvatting": "unsetIfValue==",
    "metadata.verzoek.termijnoverschrijding": "unsetIfValue==",
    "metadata.verzoek.ontvangstdatum": "unsetIfValue==",
    "metadata.besluitdatum": "unsetIfValue==",
    "metadata.verzoek.besluitdatum": "unsetIfValue==",
    "metadata.verzoek.informatieverzoek": "jsonToArray",
    "metadata.verzoek.inventarisatielijst": "jsonToArray",
    "metadata.verzoek.besluit": "jsonToArray",
    "metadata.verzoek": "unsetIfValue==",
    "categorie": "unsetIfValue==values.attribute&#46;woo_categorie",
    "publicatiedatum": "unsetIfValue=="
  }
}
