{
  "title": "Sitemap",
  "$id": "https://commongateway.nl/mapping/woo.sitemap.mapping.json",
  "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
  "version": "0.0.2",
  "passTrough": false,
  "mapping": {
    "loc": "{{ app.request.getSchemeAndHttpHost()|trim('/', 'right') }}/{{ object.id }}",
    "lastmod": "{{ \"now\"|date(\"Y-m-d H:i:s\") }}"
  }
}
