{
  "title": "OpenWooToWoo",
  "$id": "https://commongateway.nl/mapping/woo.openWooToWoo.mapping.json",
  "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
  "version": "0.0.2",
  "passTrough": false,
  "mapping": {
    "_sourceId": "UUID",
    "id": "Object_ID",
    "behandelendBestuursorgaan.oidn": "{% if behandelendBestuursorgaan.oidn|default %}{{ behandelendBestuursorgaan.oidn }}{% else %}unknown{% endif %}",
    "behandelendBestuursorgaan.naam": "behandelendBestuursorgaan.naam",
    "categorie": "categorie",
    "titel": "{% if Titel|default %}{{ Titel }}{% endif %}",
    "samenvatting": "{% if Samenvatting|default %}{{ Samenvatting }}{% endif %}",
    "ontvangstdatum": "{% if Ontvangstdatum|default %}{{ Ontvangstdatum }}{% endif %}",
    "publicatiedatum": "{% if Ontvangstdatum|default %}{{ Ontvangstdatum }}{% endif %}",
    "metadata.besluitdatum": "{% if Besluitdatum|default %}{{ Besluitdatum }}{% endif %}",
    "behandelstatus": "{% if Behandelstatus|default %}{{ Behandelstatus }}{% endif %}",
    "termijnoverschrijding": "{% if Termijnoverschrijding|default %}{{ Termijnoverschrijding }}{% endif %}",
    "urlInformatieverzoek": "{% if URL_informatieverzoek|default %}{{ URL_informatieverzoek }}{% endif %}",
    "urlInventarisatielijst": "{% if URL_inventarisatielijst|default %}{{ URL_inventarisatielijst }}{% endif %}",
    "urlBesluit": "{% if URL_besluit|default %}{{ URL_besluit }}{% endif %}",
    "informatieverzoek": "{% if Ontvanger_informatieverzoek|default %}{{ Ontvanger_informatieverzoek }}{% endif %}",
    "besluit": "{% if Besluit|default %}{{ Besluit }}{% endif %}",
    "bijlagen": "[{% set index=0 %}{% if Bijlagen|default %}{% for bijlage in Bijlagen %}{% if index != 0 %},{% endif %}{{ map('https://commongateway.nl/mapping/woo.openWooBijlageToWooBijlage.mapping.json', bijlage)|json_encode }}{% set index=index+1 %}{% endfor %}{% endif %}]",
    "themas.0.hoofdthema": "{% if Themas|default %}{% for thema in Themas %}{% if thema.Hoofdthema|default %}{{ thema.Hoofdthema~', ' }}{% endif %}{% endfor %}{% endif %}"
  },
  "cast": {
    "id": "string",
    "themas.0.hoofdthema": "unsetIfValue==",
    "themas.0": "unsetIfValue==",
    "informatieverzoek": "jsonToArray",
    "inventarisatielijst": "jsonToArray",
    "besluit": "jsonToArray",
    "bijlagen": "jsonToArray",
    "portalUrl": "unsetIfValue==",
    "titel": "unsetIfValue==",
    "beschrijving": "unsetIfValue==",
    "samenvatting": "unsetIfValue==",
    "ontvangstdatum": "unsetIfValue==",
    "termijnoverschrijding": "unsetIfValue==",
    "behandelstatus": "unsetIfValue==",
    "metadata.besluitdatum": "unsetIfValue==",
    "categorie": "unsetIfValue==",
    "publicatiedatum": "unsetIfValue==",
    "urlInformatieverzoek": "unsetIfValue==",
    "urlInventarisatielijst": "unsetIfValue==",
    "urlBesluit": "unsetIfValue=="
  }
}
