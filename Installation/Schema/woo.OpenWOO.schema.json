{
    "title": "OpenWOO",
    "$id": "https://commongateway.nl/pdd.openWOO.schema.json",
    "$schema": "https://docs.commongateway.nl/schemas/Entity.schema.json",
    "version": "0.0.7",
    "type": "object",
    "description": "",
    "properties": {
        "oidn": {
            "type": "string",
            "description": "The public available code for the designated organisation (municipality) this object is meant for."
        },
        "UUID": {
            "type": "string"
        },
        "ID": {
            "type": "string"
        },
        "Object_ID": {
            "type": "integer"
        },
        "Portal_url": {
            "type": "string",
            "format": "url"
        },
        "Behandelend_bestuursorgaan": {
            "type": "string"
        },
        "Ontvanger_informatieverzoek": {
            "type": "string"
        },
        "Volgnummer": {
            "type": "string"
        },
        "Titel": {
            "type": "string"
        },
        "Beschrijving": {
            "type": "string"
        },
        "Samenvatting": {
            "type": "string"
        },
        "Categorie": {
            "type": "string",
            "required": true
        },
        "Verzoeker": {
            "type": "string"
        },
        "Ontvangstdatum": {
            "type": "string",
            "description": "format: Y-m-d"
        },
        "Besluitdatum": {
            "type": "string",
            "description": "format: Y-m-d"
        },
        "Publicatiedatum": {
            "type": "string",
            "required": true
        },
        "Behandelstatus": {
            "type": "string"
        },
        "Besluit": {
            "type": "string"
        },
        "Termijnoverschrijding": {
            "type": "string"
        },
        "URL_informatieverzoek": {
            "type": "string"
        },
        "URL_inventarisatielijst": {
            "type": "string"
        },
        "URL_besluit": {
            "type": "string"
        },
        "informatieverzoek": {
            "type": "object",
            "$ref": "https://commongateway.nl/pdd.bijlage.schema.json"
        },
        "inventarisatielijst": {
            "type": "object",
            "$ref": "https://commongateway.nl/pdd.bijlage.schema.json"
        },
        "besluit": {
            "type": "object",
            "$ref": "https://commongateway.nl/pdd.bijlage.schema.json"
        },
        "Geografisch_gebied": {
            "type": "string"
        },
        "BAG_ID": {
            "type": "string"
        },
        "BGT_ID": {
            "type": "string"
        },
        "Postcodegebied": {
            "type": "string"
        },
        "Adres": {
            "type": "string"
        },
        "COORDS": {
            "type": "array",
            "items": {
                "$ref": "https://commongateway.nl/pdd.coords.schema.json"
            }
        },
        "Geografische_positie": {
            "type": "array",
            "items": {
                "$ref": "https://commongateway.nl/pdd.geografischePositie.schema.json"
            }
        },
        "Bijlagen": {
            "type": "array",
            "items": {
                "$ref": "https://commongateway.nl/pdd.bijlage.schema.json"
            }
        },
        "Themas": {
            "type": "array",
            "items": {
                "$ref": "https://commongateway.nl/pdd.thema.schema.json"
            }
        }
    },
    "required": [
        "Categorie",
        "Publicatiedatum"
    ]
}