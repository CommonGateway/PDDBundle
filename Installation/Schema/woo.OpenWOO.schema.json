{
    "title": "OpenWOO",
    "$id": "https://commongateway.nl/pdd.openWOO.schema.json",
    "$schema": "https://docs.commongateway.nl/schemas/Entity.schema.json",
    "version": "0.0.9",
    "type": "object",
    "description": "",
    "properties": {
        "id": {
            "type": "string"
        },
        "portal_url": {
            "type": "string",
            "format": "url"
        },
        "behandelend_bestuursorgaan": {
            "type": "object",
            "$ref": "https://commongateway.nl/woo.behandelendBestuursorgaan.schema.json"
        },
        "metadata": {
            "type": "object",
            "$ref": "https://commongateway.nl/woo.metadata.schema.json"
        },
        "ontvanger_informatieverzoek": {
            "type": "string"
        },
        "titel": {
            "type": "string"
        },
        "beschrijving": {
            "type": "string"
        },
        "samenvatting": {
            "type": "string"
        },
        "categorie": {
            "type": "string",
            "required": true
        },
        "ontvangstdatum": {
            "type": "string",
            "description": "format: Y-m-d"
        },
        "publicatiedatum": {
            "description": "Publicatiedatum van een Woo object is nooit in de toekomst.",
            "type": "string",
            "maxDate": "now",
            "required": true
        },
        "behandelstatus": {
            "type": "string"
        },
        "Besluit": {
            "type": "string"
        },
        "termijnoverschrijding": {
            "type": "string"
        },
        "URL_informatieverzoek": {
            "type": "string"
        },
        "URL_inventarisatielijst": {
            "type": "string"
        },
        "URL_besluit": {
            "type": "string"
        },
        "informatieverzoek": {
            "type": "object",
            "$ref": "https://commongateway.nl/pdd.bijlage.schema.json"
        },
        "inventarisatielijst": {
            "type": "object",
            "$ref": "https://commongateway.nl/pdd.bijlage.schema.json"
        },
        "besluit": {
            "type": "object",
            "$ref": "https://commongateway.nl/pdd.bijlage.schema.json"
        },
        "bijlagen": {
            "type": "array",
            "items": {
                "$ref": "https://commongateway.nl/pdd.bijlage.schema.json"
            }
        },
        "themas": {
            "type": "array",
            "items": {
                "$ref": "https://commongateway.nl/pdd.thema.schema.json"
            }
        }
    },
    "required": [
        "Categorie",
        "Publicatiedatum"
    ]
}